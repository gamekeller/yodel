{
  "name": "yodel",
  "version": "1.0.0",
  "description": "Gamekeller TeamSpeak bridge/bot",
  "main": "index.js",
  "author": "Heinrich Fenkart <hnrch02@gmail.com>",
  "repository": "gamekeller/yodel",
  "license": "MIT",
  "scripts": {
    "build-dir": "rm -r build; mkdir build",
    "build": "npm run build-dir && npx babel index.js -o build/index.js && npx babel lib -d build/lib && npx babel modules -d build/modules",
    "dev": "DEBUG=yodel:* nodemon --exec \"npx babel-node\" index.js"
  },
  "dependencies": {
    "@pm2/io": "^2.4.7",
    "bluebird": "^3.5.3",
    "bull": "^1.0.0",
    "cwd": "^0.10.0",
    "debug": "^3.1.0",
    "hiredis": "^0.5.0",
    "ioredis": "^3.2.2",
    "lodash": "^4.17.11",
    "node-teamspeak": "^1.0.10"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/preset-env": "^7.2.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-function-bind"
    ]
  }
}

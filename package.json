{
  "name": "yodel",
  "version": "1.0.0",
  "description": "Gamekeller TeamSpeak bridge/bot",
  "main": "index.js",
  "author": "Heinrich Fenkart <hnrch02@gmail.com>",
  "repository": "gamekeller/yodel",
  "license": "MIT",
  "scripts": {
    "build-dir": "rm -r build; mkdir build",
    "build": "npm run build-dir && babel index.js -o build/index.js && babel lib -d build/lib && babel modules -d build/modules",
    "dev": "DEBUG=yodel:* nodemon --exec \"babel-node\" index.js"
  },
  "dependencies": {
    "bluebird": "^3.4.6",
    "bull": "^1.0.0",
    "compare-versions": "^3.0.0",
    "cwd": "^0.10.0",
    "debug": "^2.2.0",
    "hiredis": "^0.5.0",
    "ioredis": "^2.3.0",
    "lodash": "^4.15.0",
    "node-teamspeak": "^1.0.10",
    "pmx": "^0.6.4"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-preset-es2015": "^6.14.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-function-bind"
    ]
  }
}

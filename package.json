{
  "name": "yodel",
  "version": "1.0.0",
  "description": "Gamekeller TeamSpeak bridge/bot",
  "main": "index.js",
  "author": "Heinrich Fenkart <hnrch02@gmail.com>",
  "repository": "gamekeller/yodel",
  "license": "MIT",
  "scripts": {
    "build-dir": "rm -r build; mkdir build",
    "build": "npm run build-dir && babel index.js -o build/index.js && babel lib -d build/lib && babel modules -d build/modules",
    "dev": "DEBUG=yodel:* nodemon --exec \"babel-node\" index.js"
  },
  "dependencies": {
    "@pm2/io": "^2.4.7",
    "bluebird": "^3.5.3",
    "bull": "^1.0.0",
    "cwd": "^0.10.0",
    "debug": "^3.1.0",
    "hiredis": "^0.5.0",
    "ioredis": "^3.2.2",
    "lodash": "^4.17.11",
    "node-teamspeak": "^1.0.10"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-preset-env": "^1.7.0"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-function-bind"
    ]
  }
}
